{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-x64",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/FPVTracksideCore/FPVTracksideCore.csproj",
                "-c",
                "Debug",
                "-r",
                "win-x64"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "silent",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            },
            "problemMatcher": "$msCompile"
        }
    ]
}